# Example environment file for the WhatsApp chatbot

# OpenAI
OPENAI_API_KEY=your-openai-key
OPENAI_MODEL_NAME=gpt-3.5-turbo
OPENAI_MODEL_TEMPERATURE=0.7

# EvolutionAPI credentials
EVOLUTION_API_URL=http://localhost:8080
AUTHENTICATION_API_KEY=your-evolution-key
EVOLUTION_INSTANCE_NAME=instance-name

# Redis cache
CACHE_REDIS_URI=redis://redis:6379/0

# Vector store path
VECTOR_STORE_PATH=vectorstore

# Message buffering and prompts
BUFFER_KEY_SUFIX=buffer
BUFFER_TTL=3600
DEBOUNCE_SECONDS=1
AI_CONTEXTUALIZE_PROMPT=Contextualize
AI_SYSTEM_PROMPT=System prompt here

# Directory to watch for RAG files
RAG_FILES_DIR=rag_files

# Painel de im√≥veis
REAL_ESTATE_ADMIN_URL=https://example.com/admin
REAL_ESTATE_ADMIN_USERNAME=admin
REAL_ESTATE_ADMIN_PASSWORD=password

